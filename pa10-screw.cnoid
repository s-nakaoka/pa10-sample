      - { name: "SceneBar", x: 1153, priority: 0 }
    - 
      - { name: "GraphBar", x: 0, priority: 0 }
      - { name: "SimulationBar", x: 128, priority: 0 }
      - { name: "BodyBar", x: 367, priority: 0 }
      - { name: "KinematicsBar", x: 683, priority: 0 }
